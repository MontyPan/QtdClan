<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:c="urn:import:com.sencha.gxt.widget.core.client.container"
	xmlns:qtd="urn:import:us.dontcareabout.QtdClan.client.component"
	>
	
	<ui:with field="gridHLD" type="com.sencha.gxt.widget.core.client.container.HorizontalLayoutContainer.HorizontalLayoutData">
		<ui:attributes width="300" height="1" />
	</ui:with>
	<ui:with field="chartHLD" type="com.sencha.gxt.widget.core.client.container.HorizontalLayoutContainer.HorizontalLayoutData">
		<ui:attributes width="1" height="1" />
	</ui:with>
	
	<c:HorizontalLayoutContainer>
		<qtd:MemberChart ui:field="chart" layoutData="{chartHLD}" />
		<qtd:MemberGrid ui:field="grid" layoutData="{gridHLD}" />
	</c:HorizontalLayoutContainer>
</ui:UiBinder> 